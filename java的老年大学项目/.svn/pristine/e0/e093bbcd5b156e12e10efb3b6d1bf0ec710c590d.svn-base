<?xml version="1.0" encoding="utf-8"?>
<trans name="detail" desc="活动-详情">
    <snd>
        <field name="actId" desc="id"/>
        <field name="actTitle" desc="活动标题"/>
        <field name="actInfo" desc="简介"/>
        <field name="actContent" desc="活动内容"/>
        <field name="actPhoto" desc="封面"/>
        <field name="actAreaId" desc="活动区域id"/>
        <field name="actType" desc="活动类型1宣传活动、2评比类活动" type="N"/>
        <field name="actStartDate" desc="活动开始时间" type="D"/>
        <field name="actEndDate" desc="活动结束时间" type="D"/>
        <field name="actTutorUserId" desc="指导老师id"/>
        <field name="actImgs" desc="活动图片"/>
        <field name="siteId" desc="站点" required="true"/>
        <field name="catId" desc="活动分类id" required="true"/>
    </snd>
    <rcv>
        <field name="status" desc="0表示删除成功 1表示存在活动作品不能更新 2存在活动分类3存在活动成员 4存在活动成果"/>
    </rcv>
</trans>