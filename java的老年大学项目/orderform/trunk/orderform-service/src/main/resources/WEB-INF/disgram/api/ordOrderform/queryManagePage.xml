<?xml version="1.0" encoding="utf-8"?>
<trans name="queryManagePage" desc="订单-可管理列表" isPage="true">
	<snd>
		<field name="sites" desc="站点id 多个id逗号分开" />
		<field name="userId" desc="下单人id" />
		<field name="name" desc="订单名称" />
		<field name="sn" desc="订单号" />
