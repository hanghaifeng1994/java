@charset "utf-8";
/* CSS Document */
html {color: #333;font: 12px Tahoma,Arial,"SimSun", sans-serif;}
a{ color:#333; text-decoration:none;}
a:hover{ text-decoration:none; color:#8bc638;}
.bg_patterns{ background:url(images/patterns1.png) repeat;}
#front_patterns{ background:url(images/patterns2.png) repeat-x; min-height:712px; overflow:hidden;}
#wrapper{ width:980px; margin:74px auto 0;}
/*header*/
/*#top_header{ position:relative; height: 54px;}*/
#header{ height:50px; background-color: #2C2C2C;background-image: -moz-linear-gradient(center top , #333333, #222222);background-repeat: repeat-x;box-shadow: 0 2px 3px rgba(0, 0, 0, 0.5), 0 -2px 2px 2px rgba(0, 0, 0, 0.2) inset; border-bottom:4px solid #e36902; position:fixed;top:0;left:0;width:100%;z-index:9999999; filter:alpha(opacity=90);-moz-opacity:0.9; opacity: 0.9; line-height:50px;}
#header .header_box{ width:980px; margin:0 auto;}
#header .logo{ background:url(images/logo.png) left center no-repeat; height:50px; display:inline-block; float:left; font:20px/50px microsoft yahei; color:#fff; padding-left:58px;}
#header .logo1{height:50px; display:inline-block; float:left; font:20px/50px microsoft yahei; color:#fff; padding-left:58px;}
#header .small_circle{ width:8px; height:8px; background:#c2c2c2; display:inline-block; border-radius:10px; -webkit-border-radius:10px; -moz-border-radius:10px; margin:0 10px;}
#header .title{ font:20px/50px microsoft yahei; color:#fff;}
.dividing_line{ color:#cfcfcf; margin:0 8px;}
.header_box .link{ float:right; display:inline-block;}
.header_box .link a{ font:14px/50px microsoft yahei; color:#fff;}
.header_box .link a i{ background-image:url(images/icon.png); background-repeat:no-repeat; width:20px; height:20px; display:inline-block; vertical-align:text-top; margin-right:5px;}
.icon_openbook{ background-position:0 -20px;}
.icon_creditbank{ background-position:0 -40px;}
.icon_power{background-position:0 -80px;}
.icon_login{background-position:0 -100px;}
/*.nav_bar{ position: relative; z-index: 1; width: 1070px; margin: 0 auto;}*/
.nav{ background:url(images/nav.png) no-repeat; width:930px; height:78px; position:absolute; top:10px; right:-4px; padding-left:140px; z-index: 999999;}
.nav ul li{ float:left; margin:0 20px; display:inline;}
.nav ul li a{ font:bold 18px/72px microsoft yahei; color:#477902; text-shadow:1px 1px 1px #fff;}
.nav ul li .selected{ color:#fff; text-shadow:1px 1px 1px #477902;}
.nav .search{ float:right; margin:24px 30px 0 0;box-shadow:0 0 3px rgba(0, 0, 0, 0.3);-webkit-box-shadow:0 0 3px rgba(0, 0, 0, 0.3);-moz-box-shadow:0 0 3px rgba(0, 0, 0, 0.3);}
.nav .search input{ border-radius:4px 0 0 4px; -webkit-border-radius:4px 0 0 4px; -moz-border-radius:4px 0 0 4px; border:none; height:20px; width:150px; float:left;}
.nav .search button{ height:26px; width:36px; background:#ccc url(images/seaech_btn.png) no-repeat; cursor:pointer; border-radius:0 4px 4px 0; }
.nav .title {font: 20px/70px microsoft yahei;color: #fff;}
/*content*/
#content{ background:#fff; width:960px; margin:20px auto; min-height:600px; position: relative; border-radius:4px; -webkit-border-radius:4px; -moz-border-radius:4px; box-shadow:0 0 10px rgba(0, 0, 0, 0.3);-webkit-box-shadow:0 0 10px rgba(0, 0, 0, 0.3);-moz-box-shadow:0 0 10px rgba(0, 0, 0, 0.3);}
.sliderbox{ height:260px; width:700px; position:relative; float:left;}
.sliderbox .slider li img{ border-radius:4px 0 0 0; width:700px; height:260px;}
.sliderbox .control{ position:absolute; bottom:10px; right:10px;}
.sliderbox .control li{ width:12px; height:12px; float:left; background-color:#000; cursor:pointer; margin-right:5px; opacity:0.6; filter:alpha(opacity=60);}
.sliderbox .control .selected{ background-color:#8bc638; opacity:1; filter:alpha(opacity=100);}
.loginsection{ background-color:#555; height:140px; width:220px; margin-left:700px; border-radius:0 4px 0 0; padding:100px 20px 20px 20px; color:#fff;}
.loginsection h1{ font:20px/30px microsoft yahei; text-align:center; margin-bottom:20px;}
.loginsection .loginbtn{ border-radius:4px;-webkit-border-radius:4px; -moz-border-radius:4px; background-color:#8bc638; border-bottom:3px solid #327a00; width:170px; height:40px; font:18px microsoft yahei; color:#fff; cursor:pointer;}
.loginsection .down{ font-size:14px; text-align:center; color:#fff; display:block; margin-top:10px; text-decoration:underline;}
.loginsection .avatar{ float:left; margin-right:20px;}
.loginsection .avatar img{ width:60px; height:60px; border-radius:6px;-webkit-border-radius:6px; -moz-border-radius:6px;}
.userinfo{ margin-left:80px; width:140px;}
.userinfo .name{ font-family: microsoft yahei; margin-bottom:5px; font-size:14px;}
.learning_btn{border-radius:4px;-webkit-border-radius:4px; -moz-border-radius:4px; background-color:#fd6a15; border-bottom:3px solid #b44504; width:110px; height:32px; font:14px microsoft yahei; color:#fff; cursor:pointer;}
.personal_info{ margin-top:25px; overflow:hidden;}
.personal_info li{ float:left; border-right:1px dotted #ccc; margin-right:18px; padding-right:18px;}
.personal_info li a{ color:#fff; }
.personal_info li.last{ border-right:none; margin-right:0; padding-right:0;}
.text_count{ font-size:18px; color:#fd6a15; font-family:Arial; margin-top:6px; text-align:center;}
.index_main{ margin:10px 20px 20px;}
.index_main .hd{ border-bottom:3px solid #8bc638;}
.index_main .hd h2{ font:18px/40px microsoft yahei; color:#8bc638;}
.icon_book{ background:url(images/icon_book.png) no-repeat; width:24px; height:21px; display:inline-block; vertical-align:middle; margin-right:5px;}
.index_main .hd .more{ float:right; font-size:12px;}
.index_main .bd{ border:1px solid #ccc; border-top:none; min-height:400px;}
.limit_free{ width:199px; height:416px; border-right:1px solid #ccc; float:left;}
.limit_free .title{ background:#8bc638; width:201px; height:40px; font:16px/40px microsoft yahei; color:#fff; text-align:center; margin-left:-1px;}
.limit_free .info{ border-bottom:1px solid #ccc; background:#f5f5f5 url(images/icon_square.png) 10px center no-repeat; height:34px; padding-left:20px;}
.limit_free ul li a{ width:170px; vertical-align:top; font:14px/34px microsoft yahei;}
.limit_free ul .cur{ height:236px; background:#fff;}
.limit_free ul .cur a{ font-weight:bold; color:#8bc638;}
.limit_free ul .cur .bookcover{ width:164px; margin:10px auto 0; overflow:hidden;}
.btn_primary{border-radius:4px;-webkit-border-radius:4px; -moz-border-radius:4px; background-color:#fd6a15; box-shadow:0 1px 2px rgba(0,0,0,0.2); padding:0 10px; height:34px; font:16px/34px microsoft yahei; color:#fff; cursor:pointer;}
.book_recommend{ float:left; width:637px; padding:0 20px; height:450px; border-right:1px solid #ccc;}
.book_recommend .tabs{ overflow:hidden; margin-bottom:20px;}
.book_recommend .tabs li{ float:left; margin:0 8px; display:inline;}
.book_recommend .tabs li a{ font:14px/36px microsoft yahei;}
.book_recommend .tabs li i{ margin:0 12px; line-height:36px;}
.book_recommend .tabs li .selected{ background-color:#8bc638; border-radius:0 0 4px 4px; height:36px; line-height:36px; width:90px; color:#fff; display:inline-block; text-align:center;}
.bookcover{ overflow:hidden;}
.bookcover li{ float:left; width:104px; margin:0 25px 20px; display:inline;}
.bookcover li .img{ background:url(images/bookbg.png) no-repeat; width:104px; height:128px;}
.bookcover li .img img{ margin:4px 0 0 4px;}
.bookcover li h4{font:normal 14px/18px microsoft yahei; margin-top:8px; padding-left:4px;}
.bookcover li h4 a:hover{ text-decoration:none; color:#8bc638;}

.rank_side{ width:200px; float:right; padding:0 20px;}
.rank_side h3{ font:16px/50px microsoft yahei;}
.rank_side ol li{border-bottom: 1px dotted #D6D6D6; height:36px; line-height:36px;}
.rank_side ol li a{ width:170px; vertical-align:middle;}
.rank_side ol li i{margin-right: 5px;font-size: 18px;display: inline-block;font-family: Arial; font-style:italic; vertical-align:middle;}
.rank_side ol .top i{ color:#fd6a15;}
.rank_side ol li a:hover{ text-decoration:none; color:#8bc638;}

.course_category { background:url(images/course_category.png) no-repeat; height:98px; margin-top:10px;}
.course_category a{ width:160px; float:left; margin-right:10px; display:inline-block; height:88px; font:20px microsoft yahei; color:#8bc638; padding:10px 0 0 16px;}
.course_category a.last{ margin-right:0;}

.icon_study{ background:url(images/icon_study.png) no-repeat; width:36px; height:32px; display:inline-block; vertical-align:middle; margin-right:5px;}
.image_content{ margin-top:10px; height:230px;}
.image_list_left{ width:294px; height:198px; border:6px solid #f2f2f2; padding:10px; float:left;}
.image_list_left h6{ text-align:center; font:16px/28px microsoft yahei;}
.image_list_left h6:hover{ text-decoration:none; color:#8bc638;}

.image_list_right{ float:right; width:582px;}
.image_link{ position:relative; display:inline-block; width:190px; height:110px; margin:0 0 10px 4px; float:left;}
.image_mask{ position:absolute; height:28px; background:#000; left:0; bottom:0; width:100%;opacity: 0.5;filter: alpha(opacity=50);}
.image_text{position:absolute; line-height:28px; left:0; bottom:0; width:100%; color:#fff; text-align:center;}
.image_link:hover .image_mask{background-color:#8bc638; opacity:1; filter:alpha(opacity=100);}
.find_btn{ background:#8bc638 url(images/find_btn.png) no-repeat;}
.find_btn:hover{ background-color:#74bc0f;}

/*自学课堂*/
.main{ margin:100px 20px 20px;}
.aside{ width:180px; float:left; margin-right:20px;}
.sidebar_list{ border-radius:4px; border:1px solid #ddd; background:#f8f8f8;}
.sidebar_list li.menu{ font:14px/40px microsoft yahei; border-bottom:solid 1px #e7e9ec; position:relative; text-indent:24px;}
.sidebar_list li i{ background:url(images/icon_aside.png) 0 -20px no-repeat; width:20px; height:20px; display:inline-block; vertical-align:middle; position:absolute; right:10px; top:10px;}
.sidebar_list li .active i{ background-position:top center;}
.sidebar_list li .active a{ color:#8bc638;}
.sidebar_list li ul{  padding:5px 0 5px 40px; background:#fff; border-top:solid 1px #e7e9ec; text-align:left;}
.sidebar_list li ul li{ background:url(images/icon_aside.png) 0 -40px no-repeat; line-height:30px;}
.sidebar_list li ul li a{ font-size:12px;}
.sidebar_list li.last{ border-bottom:none;}
.right_content{ margin-left:200px;}
.mod_search_select {height: 36px;border: 1px solid #dcdcdc;background-color: #fefefe;background-image: -moz-linear-gradient(top, #fefefe, #f1f1f1);background-image: -ms-linear-gradient(top, #fefefe, #f1f1f1);background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#fefefe), to(#f1f1f1)); background-image: -webkit-linear-gradient(top, #fefefe, #f1f1f1);
background-image: -o-linear-gradient(top, #fefefe, #f1f1f1);background-image: linear-gradient(top, #fefefe, #f1f1f1);background-repeat: repeat-x;filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#fefefe', endColorstr='#f1f1f1', GradientType=0);filter: progid:dximagetransform.microsoft.gradient(enabled=false);}
.favoi{ background:url(images/icon_det2.png) 0 -40px no-repeat; height:20px; vertical-align:middle; display:inline-block; padding-left:22px;}
.mod_search_select li { float:left;border-right: 1px solid #dfdfdf; box-shadow: inset -1px 0 1px #fff;}
.mod_search_select li a{ padding:0 16px; font:14px/36px microsoft yahei; display:inline-block;}
.mod_search_select li i{ background:url(images/icon_arrow.png) no-repeat; width:14px; height:12px; display:inline-block; vertical-align:middle;}
.mod_search_select .selected a{ color:#FD6A15;}
.mod_search_select .selected i{ background-position:0 -12px;}
.bookshelf{ background:url(images/bookshelf.png) left bottom no-repeat; width:720px; margin-top:20px; overflow:hidden; height:143px;}
.booklist li{ width:auto; margin:0 40px; *margin:0 34px; position:relative;}
.booklist li .img{ float:left;}
.booklist li .paper{ height:96px; background:#fbfaf0;box-shadow:0 0 2px rgba(0,0,0,0.2); margin-left:98px; position:relative; z-index:1; margin-top:16px; *left:-5px;}
.paper_1{width:180px;}
.booklist li h4{ padding:10px 8px 0;}
.icon_xm{ background:url(images/xm.png) no-repeat; width:36px; height:36px; margin-right:10px!important; display:inline-block; vertical-align:middle;}
.icon_coin{ background:url(images/icon_coin.png) no-repeat; width:20px; height:20px; display:inline-block; vertical-align:middle; margin-right:5px;}
.booklist li .price{ position:absolute; bottom:10px; left:10px;}
.price{ color:#999; font-family:microsoft yahei; line-height:20px;}
.price b{font-size:16px; color:#fd6a15; vertical-align:middle;}
.price .invalid{ text-decoration:line-through; vertical-align:middle;}


/*翻页*/
.pager{ background:#f9f9f9; height:20px; padding:14px 0; line-height:20px; margin:20px 0; text-align:center; font-family:microsoft yahei;}
.pager .prev{ background:url(images/pager_arrow.png) left top no-repeat; padding-left:10px; vertical-align:top;}
.pager .next{ background:url(images/pager_arrow.png) right bottom no-repeat; padding-right:10px; vertical-align:top;}
.pager .first{ padding-right:12px; vertical-align:top;}
.pager .end{ padding-left:12px; vertical-align:top;}
.pager .nums{ margin:0 10px;}
.pager .nums a{ background:#cbcbcb; border-radius:10px; padding:0 6px; display:inline-block; text-align:center; font-size:14px; margin:0 5px; color:#fff;}
.pager .nums .selected{ background:#8bc638;}
.pager .nums a:hover{ background:#8bc638;}

/*课堂详细页*/
.article{ width:680px; float:left; margin-right:20px;}
.det_block .img_block{ float:left; margin-right:20px; background:url(images/bookbg_big.png) no-repeat; width:154px; height:193px;}
.det_block .img_block img{ margin:4px 0 0 4px;}
.det_info{ margin-left:174px;}
.det_info h2{ font:18px microsoft yahei;}
.det_info .price{ margin:15px 0 5px;}
.det_info .remain_time{ margin-left:20px; color:#fd6a15; font-weight:bold; vertical-align:middle;}
.det_info .remain_time i{ background:url(images/icon_det.png) no-repeat; width:20px; height:20px; vertical-align:middle; display:inline-block; margin-right:5px;}
.det_info .info { overflow:hidden; border-bottom:1px dotted #ccc; padding:10px 0;}
.det_info .info li{ line-height:28px;}
.det_info .info li .label{ color:#999;}
.det_info .rate{ margin-left:36px; line-height:normal; padding-top:7px; }
.star{ display: inline-block; vertical-align: middle;}
.star span{ float:left; background:url(images/star.png) no-repeat; width:16px; height:16px; display:inline-block;}
.star span.no{ background-position:0 -16px;}
.det_info .rate .link{ background:url(images/icon_det.png) 0 -20px no-repeat; line-height:20px; display:inline-block; float:right; padding-left:22px;}
.complete_courses{ border:1px solid #ddd; background:#fff; border-radius:4px; margin:30px 0 20px; padding:0 10px; height:30px; line-height:30px; font-size:14px; font-weight:bold; color:#888; text-align:center;letter-spacing: 1px; box-shadow:0 1px 1px rgba(0, 0, 0, 0.1), 0 0 10px rgba(0, 0, 0, 0.1) inset;}
.complete_courses b{ font-size:16px; color:#fd6a15; margin:0 5px;}
.icon_book_white{ background:url(images/icon1.png) no-repeat; width:30px; height:30px; display:inline-block; vertical-align:top;}
.icon_try{ background:url(images/icon1.png) 0 -30px no-repeat; width:30px; height:30px; display:inline-block; vertical-align:top;}
.icon_comment{ background:url(images/icon1.png) 0 -90px no-repeat; width:30px; height:30px; display:inline-block; vertical-align:top;}
.btn_info{border-radius:4px;-webkit-border-radius:4px; -moz-border-radius:4px; border:1px solid #d4d4d4; background-color: #fefefe;background-image: -moz-linear-gradient(top, #fefefe, #f1f1f1);background-image: -ms-linear-gradient(top, #fefefe, #f1f1f1);background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#fefefe), to(#f1f1f1)); background-image: -webkit-linear-gradient(top, #fefefe, #f1f1f1);
background-image: -o-linear-gradient(top, #fefefe, #f1f1f1);background-image: linear-gradient(top, #fefefe, #f1f1f1);background-repeat: repeat-x;filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#fefefe', endColorstr='#f1f1f1', GradientType=0);filter: progid:dximagetransform.microsoft.gradient(enabled=false); box-shadow:0 1px 2px rgba(0,0,0,0.1); padding:0 10px; height:34px; font:16px/34px microsoft yahei; cursor:pointer;}

.favo{ background:url(images/icon_det.png) 0 -40px no-repeat; height:20px; vertical-align:middle; display:inline-block; padding-left:22px;}
.det_ops{ border:1px solid #ddd;box-shadow: 1px 1px 1px rgba(0, 0, 0, 0.07); border-radius:2px; background:#fff; padding:10px; overflow:hidden;}
.det_ops ul li label{ color:#999; vertical-align:inherit;}
.det_ops ul li{ line-height:20px; margin:5px 0;}
.det_ops .section{ color:#019fda; font-weight:bold;}
.det_ops .total{ background:url(images/icon_det.png) 0 -140px no-repeat; padding-left:20px; display:block;}
.det_ops .learned{ background:url(images/icon_det.png) 0 -160px no-repeat; padding-left:20px; display:block;}
.introbox{ margin-top:25px; border:1px solid #e9e9e9;}
.introbox .hd{ background:#f6f6f6; height:34px; border-bottom:1px solid #e9e9e9; font:14px/34px microsoft yahei; padding-left:20px;}
.introbox .bd{ padding:10px;}
.introbox .bd .txt{ line-height:30px; text-indent:24px;}
.introbox .hd ul li{ float:left; margin:0 10px; padding:0 10px;}
.introbox .hd ul li.selected{ background:#fff; border-top:2px solid #8bc638; border-left:1px solid #e9e9e9; border-right:1px solid #e9e9e9; position:relative; margin-top:-1px;}
.introbox .hd ul li.selected a{ color:#8bc638;}
.comment_list li{ overflow:hidden; border-bottom:1px dotted #ccc; padding:20px 0;}
.comment_list li .head_pic{ float:left; margin-right:10px;}
.comment_box{ margin-left:40px;}
.comment_info{ overflow:hidden; margin-bottom:10px;}
.comment_info .date{ float:right; color:#999;}
.comment_info .name{ float:left; margin-right:10px;}
.comment_short{ color:#666; line-height:20px;}
.right_side{ float:right; width:220px;}
.bookinfo{ background:#F6F6F6; height:30px; line-height:30px; padding-left: 10px; text-align:center; margin-bottom:15px;}
.bookinfo span{ margin-right:10px;}
.bookinfo span i{ display:inline-block; line-height:20px; padding-left:20px; vertical-align:middle;}
.bookinfo .courses i{ background:url(images/icon_det.png) 0 -60px no-repeat;}
.bookinfo .click i{ background:url(images/icon_det.png) 0 -80px no-repeat;}
.mod_list{ border:1px solid #e0e0e0; padding:10px; margin-bottom:15px;}
.mod_list .tit{ font-size:14px; font-weight:bold; margin-bottom:10px; }
.platform{ background:url(images/platform.png) no-repeat; width:198px; height:74px;position:relative;}
.platform a{ width:82px; height:74px; display:block; position:absolute; top:0;}
.platform .iphone{ left:0;}
.platform .android{ right:0;}
.mod_list .booklist li{ margin:5px 0;}
.paper_2{width:98px;}

/*培训教室*/
.train_list li{ border:1px solid #dcdcdc; margin-top:-1px; overflow:hidden;}
.train_list li:hover{ background: #f6fbf1;}
.train_list li:hover .train_operation{ background: #f6fbf1;}
.train_list .img{ margin:10px 10px 20px; width:190px; height:110px; float:left; display:inline;}
.train_list .train_detail{ float:left; margin:10px 20px 20px 0; width:340px;}
.train_list .train_detail h3{ font-size:14px; font-weight:bold;}
.train_list .train_detail .intro{ margin:7px 0; color:#999;word-wrap: break-word; line-height:18px; height:56px; overflow:hidden;}
.train_list .train_detail .attribute span{ margin-right:10px; display:inline-block;}
.train_list .train_detail .attribute span label{ color:#999; vertical-align:inherit;}
.train_list .train_operation{ float:right; background:#f7f7f7; border-left:1px dotted #ccc; width:147px; height:100px; padding-top:40px; text-align:center;}
.icon_pen{ background:url(images/icon1.png) 0 -60px no-repeat; width:30px; height:30px; display:inline-block; vertical-align:top;}

/*培训详细*/
.img_box{ float:left; width:190px; margin-right:20px;}
.det_info2{ margin-left:210px;}
.icon_certificate{ background:url(images/icon_det.png) 0 -100px no-repeat; width:20px; height:20px; display:inline-block; vertical-align:middle;}
.course_popup{ border:2px solid #fd6a15; border-radius:4px; background:#fff; height:162px; overflow-y:scroll; position:absolute; top:28px; right:0; bottom:0; left:0; box-shadow:0 2px 2px rgba(0,0,0,0.2); padding:10px;}
.course_item li{ width: 42%; border:4px solid #eee; padding:20px; float:left; margin:10px; display: inline;}
.course_item .course_name{ font-size:14px; font-weight:bold;}
.course_item .course_time{ margin-top:20px; color:#999; border-top: 1px dotted #ddd; padding-top: 10px;}
.course_item .course_time .date{ color:#fd6a15; margin:0 5px;}
.course_item .selected{border-color: #8bc638; background: url(images/image_selected.png) right bottom no-repeat;}
.course_item .none{ width: auto; float: none; display: inherit; margin: 10px 0; padding: 12px;}
.hourglass{ background:url(images/hourglass.png) no-repeat; display:inline-block; width:16px; height:20px; vertical-align:middle;}
.full{background:url(images/full_ribbon.png) no-repeat; display:inline-block; width:17px; height:23px; vertical-align:middle;}
.booklist li .img .mark{display:inline-block;height:22px; line-height:22px; width:42px; text-align:center; position:absolute; top:4px; left:4px; color:#fff; }
.booklist .required_course{ background:#FE8C1A; }
.booklist .selective_course{background:#3679BE;}
.mr40{ margin-right:40px;}
.booklist .mb{ margin-bottom:15px;}
.booklist .btn{ background:url(images/probation_btn.png) no-repeat; width:40px; height:98px; display:inline-block; position:absolute; top:15px; right:1px; z-index:2;}
.course_filter{ border-bottom:1px solid #dcdcdc; padding-bottom:5px; margin-bottom:15px; overflow:hidden;}
.course_filter .filter_title{ color:#999;}
.course_filter li{ float:left; margin:5px; padding:0 10px; display:inline; line-height:22px;}
.course_filter li.selected{ background:#fe8d32; }
.course_filter li.selected a{ color:#fff;}
.user_list li{ background:url(images/icon_aside.png) 0 -40px no-repeat; padding-left:20px; line-height:30px; border-bottom:1px dotted #ccc;}
.user_list li i{ background: url(images/icon_det.png) 0 -120px no-repeat; padding-left:20px; height:20px; display:inline-block; vertical-align:middle; margin-left:6px;}

/*报名选课*/
.fivestep{overflow:hidden; margin-bottom:20px;}
.fivestep li{height:30px; width:180px; float:left;background-color:#e4e4e4;background-image: url(images/flow_steps_bg.png);background-repeat: no-repeat;background-position: right top;padding:0 30px 0 20px;line-height:30px;font-size:14px;}
.fivestep li strong{font:bold 16px/30px Arial;margin-right:10px;}
.fivestep li a{color:#999;}
.fivestep li.current a{color:#fff;}
.fivestep li a:hover{text-decoration:none;}
.fivestep li.current{background-color:#f60;background-image: url(images/flow_steps_bg.png);}
.fivestep li.last{background-image:none;}
.fivestep li.last_done{background-color:#ffe6ba; background-position:right -30px;}
.fivestep li.done{background-color:#ffe6ba;background-position:right -60px;}
.det_info .info .date{ color:#FD6A15;}
table {width: 100%;}
.table_box { font:12px microsoft yahei; margin-bottom:10px;}
.table_box th,.table_box td{padding:6px; line-height:24px;}
.table_box th{border-bottom:1px solid #eee;}
.table_box td{border-bottom:1px solid #eee; text-align:center;}
.table_box tbody tr:nth-child(odd) td,
.table_box tbody tr:nth-child(odd) th { background-color: #f9f9f9;}
.course_intro{ min-height:400px;}
.course_intro_popup{background:#FFE6BA; border-radius:4px; padding:5px 10px; line-height:30px; position:fixed; bottom:0; left:0; right:0; width:940px; margin:0 auto;box-shadow:1px 2px 2px rgba(0, 0, 0, 0.2) inset;}
.course_intro_popup span{ margin-right:10px;}

.table-bordered > thead > tr > th, .table-bordered > tbody > tr > th, .table-bordered > tfoot > tr > th, .table-bordered > thead > tr > td, .table-bordered > tbody > tr > td, .table-bordered > tfoot > tr > td {
    border: 1px solid #DDDDDD;
	padding: 8px;
}
.table-bordered > thead > tr > th,.table-bordered > tbody > tr > th{
	background-color: #F9F9F9;
}

.text-default{
	font:normal 24px microsoft yahei;
	color: #666;	
	text-align: center;
	margin: 20px 0;
}
.enter-link{
	font:normal 14px microsoft yahei;
	text-align: center;
}
.enter-link a{
	color: #8bc638;
}
.icon_arrow_right {
    background: url("images/arrow_right.png") no-repeat scroll 0 0 rgba(0, 0, 0, 0);
    display: inline-block;
    height: 22px;
    vertical-align: text-top;
    width: 22px;
}

/*footer*/
#footer{ background-color: #2C2C2C;background-image: -moz-linear-gradient(center top , #333333, #222222);background-repeat: repeat-x;box-shadow: 0 2px 3px rgba(0, 0, 0, 0.5), 0 0 2px 2px rgba(0, 0, 0, 0.2);filter:alpha(opacity=70);-moz-opacity:0.7; opacity: 0.7; padding: 5px 0; position: relative; z-index: 101;}
#footer p{ text-align: center; line-height: 22px; color: #fff; font-family: microsoft yahei;}

.course_evaluate_box{
	overflow:hidden;
}
.course_evaluate_list{
	width:750px;
	overflow:hidden;
	margin-right:30px;
	float:left;
	}
.course_evaluate_al dt,.course_evaluate_al dd {
	float:left;
	}
.course_evaluate_al dt{
	font-weight:bold;
	margin-right:10px;
	}
.course_evaluate_al{
	clear:both;
	overflow:hidden;
	margin-bottom:10px;
	height:18px;
	}
.stars{
	position:relative;
	background:url(images/star.png) repeat-x left bottom;
	width:80px;
	height:15px;
	}
.stars a{	
	text-indent:-999px;
	height:15px;
	overflow:hidden;
	display:inline-block;
	position:absolute;
	left:0;
	top:0;
	}
.stars a:hover{
	background:url(images/star.png) repeat-x left top;
	}
.course_evaluate_a2 dt,.course_evaluate_a2 dd{
	float:left;
	}
.course_evaluate_a2 dt{
	margin-right:10px;
	}
.course_evaluate_a2 {
	float:left;
	margin-right:15px;
	margin-bottom:5px;
	}
.stars_bar{
	background:url(images/star.png) repeat-x scroll left bottom;
	width:80px;
	height:15px;
	}
.stars_bar div{
	background:url(images/star.png) repeat-x scroll left top;
	height:15px;
	}
.course_btn_orange {
    background-color: #F7941D;
    border: 1px solid rgba(0, 0, 0, 0.1);
