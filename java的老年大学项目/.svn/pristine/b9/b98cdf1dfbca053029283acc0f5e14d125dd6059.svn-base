<?xml version="1.0" encoding="utf-8"?>
<trans name="simpleImp" desc="简单导入用户，excel第三行为数据，用户名,身份证号,手机号,邮箱,性别,生日,所属单位">
    <snd>
        <field name="fileId" desc="文件id"/>
    </snd>
    <rcv>
        <field name="addUserNum" desc="新增用户数量"/>
        <field-list name="invalidUserList" desc="无效用户列表" type="E">
            <field name="custName" desc="姓名"/>
            <field name="idcarNo" desc="身份证号码"/>
            <field name="phone" desc="手机号"/>
            <field name="email" desc="邮箱"/>
            <field name="sex" desc="性别"/>
            <field name="birthday" desc="用户生日"/>
        </field-list>
        <field-list name="userList" desc="有效用户列表" type="E">
            <field name="custId" desc="用户id"/>
            <field name="custName" desc="姓名"/>
            <field name="idcarNo" desc="身份证号码"/>
            <field name="phone" desc="手机号"/>
            <field name="email" desc="邮箱"/>
            <field name="sex" desc="性别"/>
            <field name="birthday" desc="用户生日"/>
        </field-list>
    </rcv>
</trans>