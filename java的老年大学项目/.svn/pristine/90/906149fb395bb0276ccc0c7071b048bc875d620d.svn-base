}
.tree .tree-item .tree-item-name {
	margin-left: 2px;
}
.tree .tree-folder,
.tree .tree-item {
	position: relative;
}
.tree .tree-folder:before,
.tree .tree-item:before {
	display: inline-block;
	content: "";
	position: absolute;
	top: 14px;
	left: -13px;
	width: 18px;
	height: 0;
	border-top: 1px dotted #666;
	z-index: 1;
}
