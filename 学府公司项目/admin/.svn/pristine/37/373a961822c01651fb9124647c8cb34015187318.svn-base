<script>
    (function (window, $) {
        var pagepostdata = utils_admin.clone(apidata.postdata);
        pagepostdata.payload.custId = '';
        let pagedataone = utils_admin.setajax('POST', pagepostdata, apidata.resultbill.bilPoints);
        utils_admin.queryPage("intergralstudent", "intergralstudent_t", pagedataone);
        //分页1
        new pagination({
            pagination: $('.pagination0'),
            maxPage: 5,
            startPage: 1,
            currentPage: 1,
            totalPageCount: (pagedataone.totalCount / pagedataone.pageSize) - (pagedataone.totalCount / pagedataone.pageSize) % 1 + 1,  //总页数
            callback: function (pageNum) {
                pagepostdata.payload.page = "" + pageNum + "";
                utils_admin.queryPage("intergralstudent", "intergralstudent_t", utils_admin.setajax('POST', pagepostdata, apidata.resultbill.bilPoints));
            }
        });


        function seek() {
           var input=$("input[type=text]");
           input[0].value=pagepostdata.payload.custId;
            utils_admin.queryPage("intergralstudent", "intergralstudent_t", utils_admin.setajax('POST', pagepostdata, apidata.resultbill.bilPoints));
        }

        window.seek=seek;
    })(window, window.$)

</script>