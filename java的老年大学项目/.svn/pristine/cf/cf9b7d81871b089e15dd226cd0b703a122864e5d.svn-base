<?xml version="1.0" encoding="utf-8"?>
<trans name="detail" desc="角色信息(包括功能id)">
    <snd>
        <field name="roleId" desc="id"/>
    </snd>
    <rcv>
        <field name="roleId" desc="ID"/>
        <field name="roleName" desc="角色名称"/>
        <field name="roleEname" desc="角色英文名称" required="true"/>
        <field name="userable" desc="0否、1是"/>
        <field name="createBy" desc="创建人"/>
        <field name="createDate" desc="创建时间"/>
        <field name="updateBy" desc="更新人"/>
        <field name="updateDate" desc="更新时间"/>
        <field name="remarks" desc="备注"/>

        <field name="funcIds" desc="功能ids" comment="多个id用逗号分割"/>
    </rcv>
</trans>