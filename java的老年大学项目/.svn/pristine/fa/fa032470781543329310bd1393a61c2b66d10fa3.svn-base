            <field name="actAuditId" desc="审核id"/>
            <field name="actSignupNum" desc="报名人数"/>
            <field name="actImgs" desc="活动图片"/>
            <field name="createBy" desc="创建人" />
            <field name="createDate" desc="创建时间"/>
            <field name="catName" desc="活动分类"/>
            <field name="updateBy" desc="更新人"/>
            <field name="createBy" desc="创建人"  />
