        <field name="tsId" desc="测试id" required="true"/>
        <field name="objId" desc="班级id" required="true"/>
    </snd>
    <rcv>
        <field-list name="starDataCounts" desc="问卷调查列表" type="E">
            <field name="name" desc="题目名称"/>
