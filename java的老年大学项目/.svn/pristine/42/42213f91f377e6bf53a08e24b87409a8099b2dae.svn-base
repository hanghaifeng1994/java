<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.learnyeai.studygroup.mapper.SgpStudyGroupTalentMapper">
    <delete id="deleteByTalentAndSgpId" parameterType="SgpStudyGroupTalent">
        DELETE FROM sgp_study_group_talent  WHERE SGP_ID=#{sgpId,jdbcType=VARCHAR} and TLT_ID=#{tltId,jdbcType=VARCHAR}
    </delete>
</mapper>
