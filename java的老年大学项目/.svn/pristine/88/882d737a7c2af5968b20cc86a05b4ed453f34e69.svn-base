package com.drcl.traincore.vo;

import java.util.List;

public class QlStudentVO {
	private Long userId;//学员id
	private String userName;//学员用户名
	private String idcard;//学员身份证号码
	public List<QlCourseTeacherVO> courses;//课程列表

	public Long getUserId() {
		return userId;
	}

	public void setUserId(Long userId) {
		this.userId = userId;
	}

	public String getUserName() {
		return userName;
	}

	public void setUserName(String userName) {
		this.userName = userName;
	}

	public String getIdcard() {
		return idcard;
	}

	public void setIdcard(String idcard) {
		this.idcard = idcard;
	}

	public List<QlCourseTeacherVO> getCourses() {
		return courses;
	}

	public void setCourses(List<QlCourseTeacherVO> courses) {
		this.courses = courses;
	}
}
