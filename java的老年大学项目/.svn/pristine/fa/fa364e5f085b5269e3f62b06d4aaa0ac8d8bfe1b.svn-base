.dr-dib{
	display:inline-block;
}


/*按钮*/

.btn {
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.1);
	box-shadow:0px 0px 1px 1px rgba(255, 255, 255, 0.2) inset;
	font-family:microsoft yahei;
}

.btn-default{
box-shadow: 0px 0px 1px 0px rgba(0, 0, 0, 0.08) inset;
text-shadow: 0 1px 0 rgba(255, 255, 255, 0.8);
}

.dropdown-menu {font-size:12px;}

/* 框架结构 */
.dr-wrapper{ background-color:#248DC1; background-image:url(images/bodybk.png); background-repeat:repeat; margin-top:50px; min-height:700px;}
.dr-sidebar{ width:220px; position:absolute; top:50px; bottom:0; left:0; min-height:100%; font-family:microsoft yahei;}
.dr-container-fluid{background-color:#f5f5f5; padding:20px; min-height:850px;box-shadow: 0 -8px 6px 4px rgba(0, 0, 0, 0.2);}
.dr-tabs-panel{ background:#fff; padding:20px;box-shadow: 0 1px 2px 1px rgba(0, 0, 0, 0.1);}
hr{ border-color:#ddd; border-bottom:1px solid #fff;}

.dr-sidebar + #main {
    padding-left: 220px;
}

/*header*/
.navbar-inverse{ border-bottom：4px solid #E36902;/*background:#58B1FC; border-color:#019ec9;*/box-shadow:0 3px 2px -3px rgba(0, 0, 0, 0.3);}
.navbar-inverse .navbar-brand{ color:#fff; font-family:microsoft yahei; padding-right:50px;}
.navbar-inverse .navbar-nav > li > a{ /*color:#bfecf8;*/}
.navbar-inverse .navbar-nav > .active > a, .navbar-inverse .navbar-nav > .active > a:hover, .navbar-inverse .navbar-nav > .active > a:focus{ background-color:#5f5f5f;}
.navbar-inverse .navbar-nav > .open > a, .navbar-inverse .navbar-nav > .open > a:hover, .navbar-inverse .navbar-nav > .open > a:focus{ background-color:#5f5f5f;}
.navbar-nav.navbar-right:last-child{ margin-right:0;}
.dr-nav-toolbar li{ cursor:pointer; display:inline-block; font-size:16px; position: inherit;}
.dr-nav-toolbar li .badge{ position:absolute; top:10px; right:0; padding:3px 5px;}
.dr-nav-toolbar li a{ padding:17px 10px 0; color:#fff!important;}
.dr-badge-warning{ background-color:#ED5466;}
.dr-nav-toolbar .logout{ font-size:24px; background-color:#5F5F5F; padding:12px 20px; margin-left:20px;}
.dr-nav-toolbar .logout:hover{ background-color:#4c4c4c!important;}
.navbar .dropdown-menu{font-size:14px;}

/*用户信息*/
.dr-user-menu{ padding:20px 0 0 20px; height:90px; border-bottom:1px solid #187EB0;}
.dr-user-menu .avatar{ border:2px solid #fff; background-color: #fff; width:50px; height:50px; border-radius:50%; float:left; margin-right:10px;}
.dr-user-menu .user-info{ display:inline-block; color:#fff;}
.dr-user-menu .user-info .welcome{ font-size:12px; font-style:italic; line-height:20px;}
.dr-user-menu .user-info .username{ font-size:16px; font-weight:bold;}


/*边导航菜单*/
.dr-topmenu{ margin:0; padding:0; border-bottom: 1px solid #187EB0;}
.dr-topmenu li {
	display:block;
	position:relative;
	width:100%;
	list-style:none;
	background-color:#248DC1;
}
.dr-topmenu li:after {
	position:absolute;
	content:"";
	left:0;
	right:0;
	bottom:0;
	border-top:1px solid #187eb0
}
.dr-topmenu li:last-child:after {
	border:0
}
.dr-topmenu li.active {
	background-color:#0E6390;
}
.dr-topmenu li.active:after {
	border-color:transparent
}
.dr-topmenu li.active a {
	color:#c3c3c3
}
.dr-topmenu li.active a:after{
    border-color: rgba(0, 0, 0, 0);
    border-image: none;
    border-style: solid;
    border-width: 13px;
    content: "";
    display: block;
    position: absolute !important;
    right: -4px;
    top: 8px;
	border-right-color:#f5f5f5;	
}
.dr-topmenu li .submenu li a:after{ border-width:0;}

.dr-topmenu li.active a>.figure>[class*=" ico-"], .dr-topmenu li.active a>.figure>[class^=ico-] {
border-color:#00b6ad;
color:#00b6ad
}
.dr-topmenu li.open a {
	color:#fff
}
.dr-topmenu li.open a:hover {
	color:#fff
}
.dr-topmenu li.open a>.figure>[class*=" ico-"], .dr-topmenu li.open a>.figure>[class^=ico-] {
border-color:#c3c3c3
}
.dr-topmenu li.open a>.dr-arrow:before {
	content:"\e114";
}
.dr-topmenu li a > .dr-arrow:before {
    content: "\e114";
    font-family: 'Glyphicons Halflings';
    font-size: 10px;
}
.dr-topmenu li a > .dr-arrow:before {
    content: "\e080";
    font-family: 'Glyphicons Halflings';
    font-size: 10px;
}
.dr-topmenu li a > .dr-arrow{ line-height:30px;}
.dr-topmenu li a {
	display:table;
	position:relative;
	table-layout:fixed;
	width:100%;
	text-decoration:none;
	color:#c7e6f6;
	outline:0;
	padding:6px 25px 6px 15px
}
.dr-topmenu li a:hover {
	color:#fff;
}
.dr-topmenu li a:hover>.figure>[class*=" ico-"], .dr-topmenu li a:hover>.figure>[class^=ico-] {
border-color:#c3c3c3
}
.dr-topmenu li a>.figure {
	display:table-cell;
	table-layout:fixed;
	vertical-align:middle;
	width:40px;
	padding-left:10px;
}
.dr-topmenu li a>.figure>.hasnotification {
	position:absolute;
	left:16px
}
.dr-topmenu li a>.text {
	display:table-cell;
	table-layout:fixed;
	vertical-align:middle;
	width:100%;
	line-height:30px;
	font-weight:600;
	white-space:nowrap;
	overflow:hidden;
	text-overflow:ellipsis
}
.dr-topmenu li a>.number {
	display:table-cell;
	table-layout:fixed;
	vertical-align:middle;
	width:40px;
	font-weight:400
}
.dr-topmenu li a>.number>.badge, .dr-topmenu li a>.number>.label {
	float:right;
	max-width:40px;
	white-space:nowrap;
	overflow:hidden;
	text-overflow:ellipsis
}
.dr-topmenu li>.submenu {
	position:relative;
	overflow:hidden;
	background-color:#389BCF 
}
.dr-topmenu li>.submenu:after {
	position:absolute;
	content:"";
	left:32px;
	top:0;
	bottom:0;
	border-left:1px dashed #0E6390
}
.dr-topmenu li>.submenu li{ background:none;}
.dr-topmenu li>.submenu li a {
	color:#c7e6f6;
	padding:1px 25px 1px 15px
}
.dr-topmenu li>.submenu li a>.figure {
	width:22px;
	padding-left:0
}
.dr-topmenu li>.submenu li a>.figure>[class*=" ico-"], .dr-topmenu li>.submenu li a>.figure>[class^=ico-] {
color:#777
}
.dr-topmenu li>.submenu li a>.text {
	font-weight:400
}

.dr-topmenu li>.submenu li a:hover, .dr-topmenu li>.submenu li a:hover>.figure>[class*=" ico-"], .dr-topmenu li>.submenu li a:hover>.figure>[class^=ico-] {
color:#fff
}
.dr-topmenu li>.submenu li:after {
	z-index:1;
	width:8px;
	height:8px;
	border-radius:50%;
	background-color:#6CB7DE;
	left:-12px;
	top:13px;
	bottom:auto;
	border-color:transparent;
	-webkit-box-shadow:0 0 0 2px #007dbe;
	box-shadow:0 0 0 2px #007dbe
}
.dr-topmenu li>.submenu li.active {
	background-color:transparent
}
.dr-topmenu li>.submenu li.active:after {
	background-color:#fff
}
.dr-topmenu li>.submenu li.active a {
	color:#fff
}
.dr-topmenu li>.submenu li.active a>.text {
	font-weight:600
}

.dr-topmenu li>.submenu .submenu {
	overflow:hidden;
	padding-left:10px
}
.dr-topmenu li>.submenu .submenu li:after, .dr-topmenu li>.submenu .submenu:after {
	display:none
}
.dr-topmenu li>.submenu .submenu li.active a {
	color:#c3c3c3
}
.dr-topmenu li>.submenu .submenu li.active a>.text {
	font-weight:700
}

.dr-topmenu li>.submenu .submenu li a {
	color:#777;
	padding-top:0;
	padding-bottom:0
}
.dr-topmenu li>.submenu .submenu li a:hover {
	color:#c3c3c3
}
.dr-topmenu li>.submenu .submenu li a>.text {
	font-weight:400
}

/*面包屑*/
.dr-breadcrumb{ list-style:none; margin:-20px -20px 10px; padding:0 0 0 10px; font-size:12px; background:#fff; border-bottom:1px solid #ddd;}
.dr-breadcrumb li{  display:inline-block; padding:8px 0;}
.dr-breadcrumb > li + li:before { color: #CCC; content: "/ "; padding: 0 5px;}
.dr-breadcrumb li a{ color:#999; margin:0 2px;}
.dr-breadcrumb li a:hover{text-decoration:none;}
.dr-breadcrumb li.active{ font-weight:bold;}
.dr-breadcrumb li:last-child{ background-image:none;}

/*页面标题*/
.dr-page-header h3{text-shadow: 0 1px 0 #fff; color:#666;}


/*搜索条*/
.dr-searchbar{ border:1px solid #ddd; background:#fbfbfb; border-radius:3px; padding:5px 10px; font-size:12px; margin-bottom:10px;}

/*table filter*/
.dr-table-bordered{ border-top:1px solid #ddd;}
.dr-table-bordered thead{ background:#FDFDFD;}
.dr-table-bordered tr:hover{background:#FDFDFD;}
.dr-sorting-desc{ background:url(images/sort-desc.png) left center no-repeat; cursor:pointer;display: inline-block;height: 19px;vertical-align: middle;width: 19px;}
.dr-sorting-asc{ background:url(images/sort-asc.png) left center no-repeat; cursor:pointer;display: inline-block;height: 19px;vertical-align: middle;width: 19px;}
.dr-sorting{background:url(images/sort-both.png) left center no-repeat; cursor:pointer;display: inline-block;height: 19px;vertical-align: middle;width: 19px;}

/*表格筛选的翻页*/
.dr-btn-toolbar{ padding:10px; border-bottom:1px solid #ddd;}
/*.dr-panel-title{ margin:0; color:#999;}*/
.dr-panel-footer{ border-top:1px solid #ddd; padding:10px;}
.dr-panel-footer .datatables-info{ text-align:right; line-height:30px;}
.panel{ border-bottom:2px solid #cdcdcd;}
.panel .table {margin-bottom: 0;}
.dr-page {
	float:left;
	height:30px;
}
.dr-page ul {
  display: inline-block;
  *display: inline;
  *zoom: 1;
  margin:0;
  padding:0;
  -webkit-border-radius: 3px;
  -moz-border-radius: 3px;
  border-radius: 3px;
  -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
  -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
  box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05);
  background:#fff;
}
.dr-page li {
  display: inline;
  float: left;
  border: 1px solid #ccc;
  border-left-width: 0;
  height:30px;
  line-height: 30px;
  font-size:12px;
}
.dr-page li input{ width:50px; height:24px; display:inline-block; padding:0; vertical-align:top; margin-top:2px;}
.dr-page a {
	padding: 0 14px;
	display:inline-block;
	color:#666;
	height:28px;
}
.dr-page .text{padding: 0 14px;}
.dr-page a:hover,
.dr-page .active a {
  background-color: #f5f5f5;
  text-decoration:none;

}
.dr-page .active a {
  color: #999999;
  cursor: default;
}
.dr-page .disabled span,
.dr-page .disabled a,
.dr-page .disabled a:hover {
  color: #999999;
  background-color: transparent;
  cursor: default;
}
.dr-page li:first-child  {
  border-left-width: 1px;
  -webkit-border-radius: 3px 0 0 3px;
  -moz-border-radius: 3px 0 0 3px;
  border-radius: 3px 0 0 3px;
}
.dr-page li:last-child  {
  -webkit-border-radius: 0 3px 3px 0;
  -moz-border-radius: 0 3px 3px 0;
  border-radius: 0 3px 3px 0;
}
.dr-page .btn-disabled{ opacity: 0.3;filter:Alpha(Opacity=30);cursor: default;}
.dr-page .btn-disabled:hover { background-color:inherit;}

/*分页*/
.dr-pagination{ display:block; text-align:center;}
.dr-pagination li a{ float:none; margin-left:-5px;}


/*组合搜索*/
.dr-form-inline .form-control{ width:auto;}
.dr-form-inline .form-group{ margin:5px 6px;}
.dr-form-inline .form-group span{ margin:0 5px;}

/*表单*/
.dr-form-bordered .form-group{ border-bottom:1px dashed #ddd; padding:15px 0; margin:0;}
.dr-form-title{ box-shadow:0 3px 3px -2px rgba(0, 0, 0, 0.1); padding:10px 0;}
.control-label{ color:#6F6F6F;}
.help-block{ color:#888;}
.dr-text-info{ padding-top: 7px; margin-bottom: 0;}
.panel-footer{ border:none; background-color:#F8F8F8;}
.panel-footer .form-group{ border:none; margin-bottom:0; padding-bottom:0;}

/*tabs*/
.panel-heading{ padding:0 15px;}
.panel-heading > .panel-title {
    font-size: 14px;
    font-weight: 600;
}
.panel-heading > .panel-title, .panel-heading > .panel-toolbar {
    display: table-cell;
    float: none !important;
    height: 40px;
    vertical-align: middle;
    width: 1%;
}
.panel-heading .panel-toolbar > .nav-tabs {
    border-bottom: 0 none;
    margin-bottom:-5px;
}
.panel-heading .panel-toolbar > .nav-tabs > li > a {
    line-height: 20px;
    padding: 7px 15px;
}
.nav-tabs > li > a{  font-family:microsoft yahei;}
.nav-tabs > li > a:hover{ background:none; border-color:#F5F5F5 #F5F5F5 #ddd; }
.nav-tabs > li.active > a, .nav-tabs > li.active > a:hover, .nav-tabs > li.active > a:focus{border-color: #DDD #DDD #fff;}
.nav > li > a:focus{ background:none;}
.nav-justified{ padding:0 15px;}


/*表单步骤向导*/
.dr-steps{background-color: #FBFBFB;border-bottom: 1px solid #ddd; padding:10px 15px; border-radius:4px 4px 0 0 ;}
.dr-steps > ul{display: table;table-layout: fixed; width: 100%; margin:0; padding:0;}
.dr-steps > ul > li{ list-style:none;display: table-cell; vertical-align: top; width: auto;}
.dr-steps > ul > li > a {
    color: #B1C2C6;
    display: block;
    position: relative;
}
.dr-steps > ul > li > a:hover,.dr-steps > ul > li > a:active,.dr-steps > ul > li > a:hover,.dr-steps > ul > li > a:focus{
	text-decoration:none;
	}
.dr-steps > ul > li > a:after {
    background-color: #DDE4E6;
    bottom: -8px;
    content: "";
    left: 17px;
    position: absolute;
    top: -8px;
    width: 2px;
}
.dr-steps > ul > li > a:after {
    bottom: auto;
    height: 2px;
    left: 0;
    right: 0;
    top: 20px;
    width: auto;
}
.dr-steps > ul > li:first-child > a:after {
    top: 0;
}
.dr-steps > ul > li:first-child > a:after {
    left: 50%;
    top: 20px;
}
.dr-steps > ul > li:last-child > a:after {
    bottom: 0;
}
.dr-steps > ul > li:last-child > a:after {
	right: 50%;
    top: 20px;
}
.dr-steps > ul > li.current > a, .dr-steps > ul > li.done > a {
    color: #859FA5;
}
.dr-steps > ul > li.error > a {
    color: rgba(237, 84, 102, 0.9);
}
.dr-steps > ul > li > a > .current-info {
    display: none;
}
.dr-steps > ul > li > a > .number, .dr-steps > ul > li > a > .title {
    display: inline-block;
    line-height: 36px;
    position: relative;
    vertical-align: top;
}
.dr-steps > ul > li > a > .number, .dr-steps > ul > li > a > .title {
    display: block;
    margin: 0 auto;
}
.dr-steps > ul > li > a > .number {
    background-color: #DDE4E6;
    border-radius: 50%;
    color: #B1C2C6;
    font-size: 14px;
    font-weight: 700;
    height: 36px;
    position: relative;
    text-align: center;
    width: 36px;
    z-index: 2;
}
.dr-steps > ul > li > a > .number:after {
    color: #FFFFFF;
    font-family: 'Glyphicons Halflings';
    font-size: 13px;
    font-weight: 400;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.1);
}
.dr-steps > ul > li > a > .title {
    font-weight: 700;
    width: auto;
}
.dr-steps > ul > li > a > .title {
    line-height: normal;
    text-align: center;
	font-size:12px;
}
.dr-steps > ul > li > a > .number + .title, .dr-steps > ul > li > a > .title + .number {
	margin-top: 5px;
}
.dr-steps > ul > li.current:first-child > a:after {
    background-image: linear-gradient(to right, #00B1E1 0px, #DDE4E6 100%);
}
.dr-steps > ul > li.current > a:after {
    background-image: linear-gradient(to right, #DDE4E6 0px, #00B1E1 50%, #DDE4E6 100%);
}
.dr-steps > ul > li.current:last-child > a:after {
    background-image: linear-gradient(to right, #DDE4E6 0px, #00B1E1 100%);
}
.dr-steps > ul > li.current > a > .number {
    background-color: #00B1E1;
    font-size: 0;
}
.dr-steps > ul > li.current > a > .number:after {
    content: "\270f";
}
.dr-steps > ul > li.done:first-child > a:after {
    background-image: linear-gradient(to right, #91C854 0px, #DDE4E6 100%);
}
.dr-steps > ul > li.done > a:after {
    background-image: linear-gradient(to right, #DDE4E6 0px, #91C854 50%, #DDE4E6 100%);
}
.dr-steps > ul > li.done:last-child > a:after {
    background-image: linear-gradient(to bottom, #DDE4E6 0px, #91C854 100%);
}
.dr-steps > ul > li.done > a > .number {
    background-color: #91C854;
    font-size: 0;
}
.dr-steps > ul > li.done > a > .number:after {
    content: "\e013";
}

/*返回顶部箭头*/
.animation.animated {
    visibility: visible;
}
.animation {
    visibility: hidden;
}
.totop {
    background-color: rgba(36, 141, 193, 0.8);
    border-radius: 50%;
    bottom: 10px;
    color: rgba(255, 255, 255, 0.8);
    display: block;
    font-size: 10px;
    height: 40px;
    line-height: 40px;
    position: fixed;
    right: 10px;
    text-align: center;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.1);
    width: 40px;
    z-index: 998;
}
.totop:hover, .totop:active, .totop:focus{
	background-color: rgba(36, 141, 193, 0.9);
	color: rgba(255, 255, 255, 0.9);
}

.animation.delay {
    animation-delay: 1.5s;
}
.animation.animating {
    animation-duration: 800ms;
    animation-fill-mode: both;
    visibility: visible;
}
.animated.hinge {
    animation-duration: 2s;
}
@-moz-keyframes bounceIn {
0% {
    opacity: 0;
    transform: scale(0.3);
}
50% {
    opacity: 1;
    transform: scale(1.05);
}
70% {
    transform: scale(0.9);
}
100% {
    opacity: 1;
    transform: scale(1);
}
}
.bounceIn {
    animation-name: bounceIn;
}
@-moz-keyframes bounceInDown {
0% {
    opacity: 0;
    transform: translateY(-2000px);
}
60% {
    opacity: 1;
    transform: translateY(30px);
}
80% {
    transform: translateY(-10px);
}
100% {
    transform: translateY(0px);
}
}
.bounceInDown {
    animation-name: bounceInDown;
}
@-moz-keyframes bounceInLeft {
0% {
    opacity: 0;
    transform: translateX(-2000px);
}
60% {
    opacity: 1;
    transform: translateX(30px);
}
80% {
    transform: translateX(-10px);
}
100% {
    transform: translateX(0px);
}
}
.bounceInLeft {
    animation-name: bounceInLeft;
}
@-moz-keyframes bounceInRight {
0% {
    opacity: 0;
    transform: translateX(2000px);
}
60% {
    opacity: 1;
    transform: translateX(-30px);
}
80% {
    transform: translateX(10px);
}
100% {
    transform: translateX(0px);
}
}
.bounceInRight {
    animation-name: bounceInRight;
}
@-moz-keyframes bounceInUp {
0% {
    opacity: 0;
    transform: translateY(2000px);
}
60% {
    opacity: 1;
    transform: translateY(-30px);
}
80% {
    transform: translateY(10px);
}
100% {
    transform: translateY(0px);
}
}
.bounceInUp {
    animation-name: bounceInUp;
}
@-moz-keyframes bounceOut {
0% {
    transform: scale(1);
}
25% {
    transform: scale(0.95);
}
50% {
    opacity: 1;
    transform: scale(1.1);
}
100% {
    opacity: 0;
    transform: scale(0.3);
}
}
.bounceOut {
    animation-name: bounceOut;
}
@-moz-keyframes bounceOutDown {
0% {
    transform: translateY(0px);
}
20% {
    opacity: 1;
    transform: translateY(-20px);
}
100% {
    opacity: 0;
    transform: translateY(2000px);
}
}
.bounceOutDown {
    animation-name: bounceOutDown;
}
@-moz-keyframes bounceOutLeft {
0% {
    transform: translateX(0px);
}
20% {
    opacity: 1;
    transform: translateX(20px);
}
100% {
    opacity: 0;
    transform: translateX(-2000px);
}
}
.bounceOutLeft {
