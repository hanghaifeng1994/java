<?xml version="1.0" encoding="utf-8"?>
<trans name="save" desc="资源文件-新增/编辑">
	<snd>
		<field name="resFielId" desc="id" />
		<field name="fileName" desc="文件名" />
		<field name="resId" desc="资源id" />
		<field name="fileId" desc="文件id" />
		<field name="fileType" desc="文件类型" />
		<field name="fileFormat" desc="文件格式" />
		<field name="fileSize" desc="大小" />
		<field name="fileSuffix" desc="后缀" />
		<field name="fileTimeLen" desc="时长" />
		<field name="siteId" desc="站点" />
		<field name="mchtId" desc="商户id" />
		<field name="mchtSchmId" desc="商户方案id" />
