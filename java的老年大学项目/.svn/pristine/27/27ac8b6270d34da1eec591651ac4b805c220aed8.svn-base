package com.learnyeai.course.api.vo;

import com.learnyeai.core.support.BaseVo;

/**
 * 课程作业
 *
 * @author twang
 */
public class CrsCourseHomeworkVo extends BaseVo {

    /**
    * ID
    */
    private String cwId;

    /**
     * 课程id
     */
    private String csId;
    /**
     * 作业id
     */
    private String hwId;

    public String getCwId() {
        return cwId;
    }

    public void setCwId(String cwId) {
        this.cwId = cwId;
    }
    public String getCsId() {
        return csId;
    }

    public void setCsId(String csId) {
        this.csId = csId;
    }
    public String getHwId() {
        return hwId;
    }

    public void setHwId(String hwId) {
        this.hwId = hwId;
    }

    public static class TF {
        public static String cwId = "cwId";  // ID
        public static String csId = "csId";  // 课程id
        public static String hwId = "hwId";  // 作业id

    }

}
