/*表单步骤向导*/

.dr-steps {
	background-color: #FBFBFB;
	border-bottom: 1px solid #ddd;
	padding: 10px 15px;
	border-radius: 4px 4px 0 0;
}
.dr-steps > ul {
	display: table;
	table-layout: fixed;
	width: 100%;
	margin: 0;
	padding: 0;
}
.dr-steps > ul > li {
	list-style: none;
	display: table-cell;
	vertical-align: top;
	width: auto;
}
.dr-steps > ul > li > a {
	color: #B1C2C6;
	display: block;
	position: relative;
}
.dr-steps > ul > li > a:hover,
.dr-steps > ul > li > a:active,
.dr-steps > ul > li > a:hover,
.dr-steps > ul > li > a:focus {
	text-decoration: none;
}
.dr-steps > ul > li > a:after {
	background-color: #DDE4E6;
	bottom: -8px;
	content: "";
	left: 17px;
	position: absolute;
	top: -8px;
	width: 2px;
}
.dr-steps > ul > li > a:after {
	bottom: auto;
	height: 2px;
	left: 0;
	right: 0;
	top: 20px;
	width: auto;
}
.dr-steps > ul > li:first-child > a:after {
	top: 0;
}
.dr-steps > ul > li:first-child > a:after {
	left: 50%;
	top: 20px;
}
.dr-steps > ul > li:last-child > a:after {
	bottom: 0;
}
.dr-steps > ul > li:last-child > a:after {
	right: 50%;
	top: 20px;
}
.dr-steps > ul > li.current > a,
.dr-steps > ul > li.done > a {
	color: #859FA5;
}
.dr-steps > ul > li.error > a {
	color: rgba(237, 84, 102, 0.9);
}
.dr-steps > ul > li > a > .current-info {
	display: none;
}
.dr-steps > ul > li > a > .number,
.dr-steps > ul > li > a > .title {
	display: inline-block;
	line-height: 36px;
	position: relative;
	vertical-align: top;
}
.dr-steps > ul > li > a > .number,
.dr-steps > ul > li > a > .title {
	display: block;
	margin: 0 auto;
}
.dr-steps > ul > li > a > .number {
	background-color: #DDE4E6;
	border-radius: 50%;
	color: #B1C2C6;
	font-size: 14px;
	font-weight: 700;
	height: 36px;
	position: relative;
	text-align: center;
	width: 36px;
	z-index: 2;
}
.dr-steps > ul > li > a > .number:after {
	color: #FFFFFF;
	font-family: 'Glyphicons Halflings';
	font-size: 13px;
	font-weight: 400;
	text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.1);
}
.dr-steps > ul > li > a > .title {
	font-weight: 700;
	width: auto;
}
.dr-steps > ul > li > a > .title {
	line-height: normal;
	text-align: center;
	font-size: 12px;
}
.dr-steps > ul > li > a > .number + .title,
.dr-steps > ul > li > a > .title + .number {
	margin-top: 5px;
}
.dr-steps > ul > li.current:first-child > a:after {
	background-image: linear-gradient(to right, #00B1E1 0px, #DDE4E6 100%);
}
.dr-steps > ul > li.current > a:after {
	background-image: linear-gradient(to right, #DDE4E6 0px, #00B1E1 50%, #DDE4E6 100%);
}
.dr-steps > ul > li.current:last-child > a:after {
	background-image: linear-gradient(to right, #DDE4E6 0px, #00B1E1 100%);
}
.dr-steps > ul > li.current > a > .number {
	background-color: #00B1E1;
	font-size: 0;
}
.dr-steps > ul > li.current > a > .number:after {
	content: "\270f";
}
.dr-steps > ul > li.done:first-child > a:after {
	background-image: linear-gradient(to right, #91C854 0px, #DDE4E6 100%);
}
.dr-steps > ul > li.done > a:after {
	background-image: linear-gradient(to right, #DDE4E6 0px, #91C854 50%, #DDE4E6 100%);
}
.dr-steps > ul > li.done:last-child > a:after {
	background-image: linear-gradient(to bottom, #DDE4E6 0px, #91C854 100%);
}
.dr-steps > ul > li.done > a > .number {
	background-color: #91C854;
	font-size: 0;
}
.dr-steps > ul > li.done > a > .number:after {
	content: "\e013";
}
/*返回顶部箭头*/

.animation.animated {
	visibility: visible;
}
.animation {
	visibility: hidden;
}
.totop {
	background-color: rgba(139, 198, 56, 0.8);
	border-radius: 50%;
	bottom: 10px;
	color: rgba(255, 255, 255, 0.8);
	display: block;
	font-size: 10px;
	height: 40px;
	line-height: 40px;
	position: fixed;
	right: 10px;
	text-align: center;
	text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.1);
	width: 40px;
	z-index: 998;
}
.totop:hover,
.totop:active,
.totop:focus {
	background-color: rgba(81, 161, 42, 0.9);
	color: rgba(255, 255, 255, 0.9);
}
.animation.delay {
	animation-delay: 1.5s;
}
.animation.animating {
	animation-duration: 800ms;
	animation-fill-mode: both;
	visibility: visible;
}
.animated.hinge {
	animation-duration: 2s;
}
@-moz-keyframes bounceIn {
	0% {
		opacity: 0;
		transform: scale(0.3);
	}
	50% {
		opacity: 1;
		transform: scale(1.05);
	}
	70% {
		transform: scale(0.9);
	}
	100% {
		opacity: 1;
		transform: scale(1);
	}
}
.bounceIn {
	animation-name: bounceIn;
}
@-moz-keyframes bounceInDown {
	0% {
		opacity: 0;
		transform: translateY(-2000px);
	}
	60% {
		opacity: 1;
		transform: translateY(30px);
	}
	80% {
		transform: translateY(-10px);
	}
	100% {
		transform: translateY(0px);
	}
}
.bounceInDown {
	animation-name: bounceInDown;
}
@-moz-keyframes bounceInLeft {
	0% {
		opacity: 0;
		transform: translateX(-2000px);
	}
	60% {
		opacity: 1;
		transform: translateX(30px);
	}
	80% {
		transform: translateX(-10px);
	}
	100% {
		transform: translateX(0px);
	}
}
.bounceInLeft {
	animation-name: bounceInLeft;
}
@-moz-keyframes bounceInRight {
	0% {
		opacity: 0;
		transform: translateX(2000px);
	}
	60% {
		opacity: 1;
		transform: translateX(-30px);
	}
	80% {
		transform: translateX(10px);
	}
	100% {
		transform: translateX(0px);
	}
}
.bounceInRight {
	animation-name: bounceInRight;
}
@-moz-keyframes bounceInUp {
	0% {
		opacity: 0;
		transform: translateY(2000px);
	}
	60% {
		opacity: 1;
		transform: translateY(-30px);
	}
	80% {
		transform: translateY(10px);
	}
	100% {
		transform: translateY(0px);
	}
}
.bounceInUp {
	animation-name: bounceInUp;
}
@-moz-keyframes bounceOut {
	0% {
		transform: scale(1);
	}
	25% {
		transform: scale(0.95);
	}
	50% {
		opacity: 1;
		transform: scale(1.1);
	}
	100% {
		opacity: 0;
		transform: scale(0.3);
	}
}
.bounceOut {
	animation-name: bounceOut;
}
@-moz-keyframes bounceOutDown {
	0% {
		transform: translateY(0px);
	}
	20% {
		opacity: 1;
		transform: translateY(-20px);
	}
	100% {
		opacity: 0;
		transform: translateY(2000px);
	}
}
.bounceOutDown {
	animation-name: bounceOutDown;
}
@-moz-keyframes bounceOutLeft {
	0% {
		transform: translateX(0px);
	}
	20% {
		opacity: 1;
		transform: translateX(20px);
	}
	100% {
		opacity: 0;
		transform: translateX(-2000px);
	}
}
.bounceOutLeft {
	animation-name: bounceOutLeft;
}
@-moz-keyframes bounceOutRight {
	0% {
		transform: translateX(0px);
	}
	20% {
		opacity: 1;
		transform: translateX(-20px);
	}
	100% {
		opacity: 0;
		transform: translateX(2000px);
	}
}
.bounceOutRight {
	animation-name: bounceOutRight;
}
@-moz-keyframes bounceOutUp {
	0% {
		transform: translateY(0px);
	}
	20% {
		opacity: 1;
		transform: translateY(20px);
	}
	100% {
		opacity: 0;
		transform: translateY(-2000px);
	}
}
.bounceOutUp {
	animation-name: bounceOutUp;
}
