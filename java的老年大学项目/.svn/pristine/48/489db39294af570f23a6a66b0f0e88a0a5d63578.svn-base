/* list */

.breadcrumb {
    margin: 20px 0;
}

.breadcrumb>.z-icon {
    padding: 4px 5px;
    border-radius: 50%;
    background-color: #398dee;
    color: #fff;
    margin-right: 5px;
}

.breadcrumb a.z-text-muted {
    margin-right: 20px;
    position: relative;
}

.breadcrumb a.z-text-muted:after {
    content: '>';
    position: absolute;
    right: -18px;
}

.label-title {
    line-height: 35px;
    background-color: #f5f5f5;
    border-top: 1px solid #ddd;
    border-bottom: 1px solid #ddd;
    padding-left: 15px;
}

.labels .table {
    width: 100%;
    text-align: center;
    display: table;
    margin-bottom: 10px;
}

.labels .table .td {
    display: table-cell;
    padding: 20px 0;
    vertical-align: middle;
    background-color: #f0f0f0;
    height: 120px;
    color: #000;
    font-size: 16px;
    position: relative;
}

.labels .table .td:after, .labels .table .td:before {
    position: absolute;
    opacity: 0;
    filter: alpha(opacity=0);
    transition: opacity ease-in-out .2s;
    -webkit-transition: opacity ease-in-out .2s;
}

.labels .table .td:after {
    content: '';
    z-index: 0;
    background-color: #000;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
}

.labels .table .td:before {
    content: '√';
    color: #fff;
    border-radius: 50%;
    width: 20px;
    height: 20px;
    display: block;
    top: 50%;
    left: 50%;
    margin-top: -10px;
    margin-left: -10px;
    border: 2px solid #fff;
    z-index: 2;
}

.labels .table .td:hover:after {
    opacity: .7;
    filter: alpha(opacity=70);
}

.labels .table .td.active:before {
    content: '√';
    position: static;
    margin: 3px auto;
    color: #000;
    border-color: #000;
}

.labels .table .td:hover:before, .labels .table .td.active:before {
    opacity: 1;
    filter: alpha(opacity=100);
}

.labels .table .td.active:hover:before {
    content: 'X';
    position: absolute;
    color: #fff;
    border-color: #fff;
    margin: -10px 0 -10px -10px;
}

.labels .table .td.color1 {
    background-color: #ffdddd;
}

.labels .table .td.color2 {
    background-color: #c9efff;
}

.labels .table .td.color3 {
