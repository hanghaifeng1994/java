            <field name="sign" desc="个性签名"/>
            <field name="phone" desc="手机号"/>
            <field name="email" desc="邮箱"/>
            <field name="qq" desc="qq"/>
            <field name="weixin" desc="微信"/>
            <field name="office" desc="所属单位"/>
            <field name="idcarNo" desc="身份证号码"/>
            <field name="sex" desc="性别"/>
            <field name="birthday" desc="用户生日"/>
            <field name="photo" desc="头像" type="res"/>
            <field name="createDate" desc="创建时间"/>
            <field name="staffType" desc="员工类型由各方案定义"/>
            <field name="custIsAdmin" desc="是否是管理员" comment="员工用户时，管理员标识：0否、1是"/>
