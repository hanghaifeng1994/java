/*首页中间内容右边部分开始-----------------*/
.indexmidr{ width:210px; float:right; background:url(../images/ico07.jpg);}

.click_bu{ text-align:center;}
.xxznbox{ background:url(../images/indexnewsbg003.jpg) no-repeat top;}
.kemu_top{ background:url(../images/ico06.jpg); height:46px; text-align:center; font-size:14px; font-family:"微软雅黑"; color:#42BDBA; line-height:32px;}
.kemu_bottom{background:url(../images/ico08.jpg); height:13px;}
.kemu{padding:8px 0 0 0;font-family:"微软雅黑"; font-size:16px; text-align:center;}
.kemu ul{float:right; margin:0 0 0 0px; padding:0px; padding-right: 14px;}
.kemu ul li{list-style:none; background:url(../images/kebg.jpg); width:182px; height:42px; line-height:40px; margin-bottom:8px; display:block;}
.kemu ul li a:link{ list-style:none; background:url(../images/kebg.jpg); width:182px; height:42px; line-height:40px;  color:#333; display:block;}
.kemu ul li a:visited{ list-style:none; background:url(../images/kebg.jpg); width:182px; height:42px; line-height:40px; color:#333; display:block;}
.kemu ul li a:hover{ list-style:none; background:url(../images/kebg.jpg); width:182px; height:42px; line-height:40px; color:#11B1CD; text-decoration:underline;display:block;}
.kemu ul li a:active{ list-style:none; background:url(../images/kebg.jpg); width:182px; height:42px; line-height:40px;  color:#333;display:block;}
/*首页中间内容右边部分结束----------------*/

