package com.learnyeai.testing.api.vo;

import com.learnyeai.core.support.BaseVo;

/**
 * 创建题库的时候，判断是否有本商户的题型，否则都要初始化一个，支持单选，多选，判断，填空
 *
 * @author twang
 */
public class TestingItemTypesVo extends BaseVo {

    /**
    * 题型ID
    */
    private String itemTypeId;

    /**
     * 题型名称
     */
    private String name;
    /**
     * 题型code(dx/ms/pd/tk)
     */
    private String itemType;
    /**
     * 商户id
     */
    private String mchtId;

    public String getItemTypeId() {
        return itemTypeId;
    }

    public void setItemTypeId(String itemTypeId) {
        this.itemTypeId = itemTypeId;
    }
    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }
    public String getItemType() {
        return itemType;
    }

    public void setItemType(String itemType) {
        this.itemType = itemType;
    }
    public String getMchtId() {
        return mchtId;
    }

    public void setMchtId(String mchtId) {
        this.mchtId = mchtId;
    }

    public static class TF {
        public static String itemTypeId = "itemTypeId";  // 题型ID
        public static String name = "name";  // 题型名称
        public static String itemType = "itemType";  // 题型code(dx/dx/pd/tk)
        public static String mchtId = "mchtId";  // 商户id

    }

}
