<?xml version="1.0" encoding="utf-8"?>
<trans name="tj" desc="课程-批量推崇/推崇">
    <snd>
    	<field name="siteId" desc="站点id" required="true"/>
        <field name="csId" desc="课程id 多个id用英文逗号隔开" required="true" />
        <field name="csTjStatus" desc="推崇状态  0/1"  required="true" />
