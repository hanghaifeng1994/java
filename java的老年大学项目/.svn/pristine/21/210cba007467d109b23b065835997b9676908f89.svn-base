<?xml version="1.0" encoding="utf-8"?>
<trans name="signup" desc="报名">
	<snd>
		<field name="custId" desc="学生id" required="true"/>
		<field name="czId" desc="班级id" required="true"/>
		<field name="csId" desc="课程id 多个逗号隔开"/>
		<field name="addCourse" desc="是否参加课程"/>
