@charset "utf-8";

/* CSS Document */

body,
h1,
h2,
h3,
h4,
h5,
h6 {
	font-family: "Microsoft YaHei", "WenQuanYi Micro Hei", sans-serif;
}
a {
	color: #51a12a;
	text-decoration: none;
}
a:hover,
a:focus {
	color: #457d16;
	text-decoration: none;
	outline: none;
	star: expression(this.onFocus=this.blur());
}
:focus {
	-moz-outline-style: none;
}
