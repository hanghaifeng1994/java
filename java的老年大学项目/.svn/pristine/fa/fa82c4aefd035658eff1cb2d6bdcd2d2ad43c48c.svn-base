package com.learnyeai.testing.model;

import com.learnyeai.learnai.support.BaseEntity;
import javax.persistence.Column;
import javax.persistence.Id;
import java.math.BigDecimal;
import java.util.Date;

/**
 * 测验
 *
 * @author twang
 */
public class TestingTest extends BaseEntity {

    /**
    * ID
    */
    @Id
    @Column(name = "TS_ID")
    private String tsId;

    /**
     * 测验名称
     */
    @Column(name = "NAME")
    private String name;
    /**
     * 是否限制测验时间
     */
    @Column(name = "LIMIT_DURATIONED")
    private String limitDurationed;
    /**
     * 是否测验限制次数
     */
    @Column(name = "LIMIT_FREQUENCYED")
    private String limitFrequencyed;
    /**
     * 测验限制分钟
     */
    @Column(name = "DURATION")
    private Integer duration;
    /**
     * 测验限制提交次数
     */
    @Column(name = "FREQUENCY")
    private Integer frequency;
    /**
     * 判分规则类型（平均分，最高分，最后一次分数）
     * 0:最高分 1:平均分 2:最后一次分数
     */
    @Column(name = "SCORE_RULE")
    private Integer scoreRule;
    /**
     * 是否显示判卷情况
     */
    @Column(name = "DISPLAY_DECIDED")
    private String displayDecided;
    /**
     * 软删除标识
     */
    @Column(name = "DELFLAG")
    private String delflag;
    /**
     * 是否有主观题
     */
    @Column(name = "HAS_SUBJECT")
    private String hasSubject;
    /**
     * 优秀线
     */
    @Column(name = "EXCELLENT_SCORE")
    private BigDecimal excellentScore;
    /**
     * 高等百分比
     */
    @Column(name = "HIGHER_PERCENT")
    private BigDecimal higherPercent;
    /**
     * 低等百分比
     */
    @Column(name = "LOWER_PERCENT")
    private BigDecimal lowerPercent;
    /**
     * 中等百分比
     */
    @Column(name = "MEDIUM_PERCENT")
    private BigDecimal mediumPercent;
    /**
     * 合格线
     */
    @Column(name = "QUALIFIED_SCORE")
    private BigDecimal qualifiedScore;
    /**
     * PC端题目显示类型（单题/整卷/分题型)
     */
    @Column(name = "PC_DISPLAY_TYPE")
    private Integer pcDisplayType;
    /**
     * 移动端题目显示类型（单体/整卷/分题型)
     */
    @Column(name = "MOBILE_DISPLAY_TYPE")
    private Integer mobileDisplayType;
    /**
     * 使用类型（练习/考试)lx/ks
     */
    @Column(name = "USER_TYPE")
    private String userType;
    /**
     * 创建人
     */
    @Column(name = "CREATE_BY")
    private String createBy;
    /**
     * 创建时间
     */
    @Column(name = "CREATE_DATE")
    private Date createDate;
    /**
     * 更新人
     */
    @Column(name = "UPDATE_BY")
    private String updateBy;
    /**
     * 更新时间
     */
    @Column(name = "UPDATE_DATE")
    private Date updateDate;
    /**
     * 删除标记
     */
    @Column(name = "DEL_FLAG")
    private String delFlag;
    /**
     * 创建站点id
     */
    @Column(name = "SITE_ID")
    private String siteId;
    /**
     * 商户id
     */
    @Column(name = "MCHT_ID")
    private String mchtId;
    /**
     * 商户方案id
     */
    @Column(name = "MCHT_SCHM_ID")
    private String mchtSchmId;
    /**
     * 创建平台
     */
    @Column(name = "PLATFORM_ID")
    private String platformId;

    public String getTsId() {
        return tsId;
    }

    public void setTsId(String tsId) {
        this.tsId = tsId;
    }
    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }
    public String getLimitDurationed() {
        return limitDurationed;
    }

    public void setLimitDurationed(String limitDurationed) {
        this.limitDurationed = limitDurationed;
    }
    public String getLimitFrequencyed() {
        return limitFrequencyed;
    }

    public void setLimitFrequencyed(String limitFrequencyed) {
        this.limitFrequencyed = limitFrequencyed;
    }
    public Integer getDuration() {
        return duration;
    }

    public void setDuration(Integer duration) {
        this.duration = duration;
    }
    public Integer getFrequency() {
        return frequency;
    }

    public void setFrequency(Integer frequency) {
        this.frequency = frequency;
    }
    public Integer getScoreRule() {
        return scoreRule;
    }

    public void setScoreRule(Integer scoreRule) {
        this.scoreRule = scoreRule;
    }
    public String getDisplayDecided() {
        return displayDecided;
    }

    public void setDisplayDecided(String displayDecided) {
        this.displayDecided = displayDecided;
    }
    public String getDelflag() {
        return delflag;
    }

    public void setDelflag(String delflag) {
        this.delflag = delflag;
    }
    public String getHasSubject() {
        return hasSubject;
    }

    public void setHasSubject(String hasSubject) {
        this.hasSubject = hasSubject;
    }
    public BigDecimal getExcellentScore() {
        return excellentScore;
    }

    public void setExcellentScore(BigDecimal excellentScore) {
        this.excellentScore = excellentScore;
    }
    public BigDecimal getHigherPercent() {
        return higherPercent;
    }

    public void setHigherPercent(BigDecimal higherPercent) {
        this.higherPercent = higherPercent;
    }
    public BigDecimal getLowerPercent() {
        return lowerPercent;
    }

    public void setLowerPercent(BigDecimal lowerPercent) {
        this.lowerPercent = lowerPercent;
    }
    public BigDecimal getMediumPercent() {
        return mediumPercent;
    }

    public void setMediumPercent(BigDecimal mediumPercent) {
        this.mediumPercent = mediumPercent;
    }
    public BigDecimal getQualifiedScore() {
        return qualifiedScore;
    }

    public void setQualifiedScore(BigDecimal qualifiedScore) {
        this.qualifiedScore = qualifiedScore;
    }
    public Integer getPcDisplayType() {
        return pcDisplayType;
    }

    public void setPcDisplayType(Integer pcDisplayType) {
        this.pcDisplayType = pcDisplayType;
    }
    public Integer getMobileDisplayType() {
        return mobileDisplayType;
    }

    public void setMobileDisplayType(Integer mobileDisplayType) {
        this.mobileDisplayType = mobileDisplayType;
    }
    public String getUserType() {
        return userType;
    }

    public void setUserType(String userType) {
        this.userType = userType;
    }
    public String getCreateBy() {
        return createBy;
    }

    public void setCreateBy(String createBy) {
        this.createBy = createBy;
    }
    public Date getCreateDate() {
        return createDate;
    }

    public void setCreateDate(Date createDate) {
        this.createDate = createDate;
    }
    public String getUpdateBy() {
        return updateBy;
    }

    public void setUpdateBy(String updateBy) {
        this.updateBy = updateBy;
    }
    public Date getUpdateDate() {
        return updateDate;
    }

    public void setUpdateDate(Date updateDate) {
        this.updateDate = updateDate;
    }
    public String getDelFlag() {
        return delFlag;
    }

    public void setDelFlag(String delFlag) {
        this.delFlag = delFlag;
    }
    public String getSiteId() {
        return siteId;
    }

    public void setSiteId(String siteId) {
        this.siteId = siteId;
    }
    public String getMchtId() {
        return mchtId;
    }

    public void setMchtId(String mchtId) {
        this.mchtId = mchtId;
    }
    public String getMchtSchmId() {
        return mchtSchmId;
    }

    public void setMchtSchmId(String mchtSchmId) {
        this.mchtSchmId = mchtSchmId;
    }
    public String getPlatformId() {
        return platformId;
    }

    public void setPlatformId(String platformId) {
        this.platformId = platformId;
    }

