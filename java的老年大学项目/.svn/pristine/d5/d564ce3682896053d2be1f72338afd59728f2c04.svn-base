        <field name="siteId" desc="站点多个id用英文逗号隔开" />
        <field name="catId" desc="分类Id" />
    </snd>
    <rcv>
        <field-list name="LIST" desc="活动详情列表" type="E">
            <field name="actId" desc="id"/>
            <field name="actTitle" desc="活动标题"/>
            <field name="actInfo" desc="简介"/>
            <field name="actContent" desc="活动内容"/>
            <field name="actPhoto" desc="封面"/>
            <field name="actAreaId" desc="活动区域id"/>
            <field name="actType" desc="活动类型1宣传活动、2评比类活动"/>
            <field name="actStartDate" desc="活动开始时间" />
            <field name="actEndDate" desc="活动结束时间" />
            <field name="actTutorUserId" desc="指导老师id"/>
