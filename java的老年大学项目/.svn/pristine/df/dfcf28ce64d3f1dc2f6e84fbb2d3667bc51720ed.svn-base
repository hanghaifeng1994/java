    public void setCtCrtSiteId(String ctCrtSiteId) {
        this.ctCrtSiteId = ctCrtSiteId;
    }

    public static class TF {
        public static String id = "id";  // ID
        public static String siteId = "siteId";  // 站点
        public static String ctId = "ctId";  // 证书id
        public static String ctCrtSiteId = "ctCrtSiteId";  // 创建站点

