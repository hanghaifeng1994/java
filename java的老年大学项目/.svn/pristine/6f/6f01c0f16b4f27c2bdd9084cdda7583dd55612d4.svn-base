<?xml version="1.0" encoding="utf-8"?>
<trans name="check" desc="证书-批量修改审核状态">
    <snd>
        <field name="ctId" desc="课程id 多个id用英文逗号隔开" required="true" />
        <field name="ctStatus" desc="审核状态 0未提交、1审核中、2审核通过、3审核失败、4不需要审核"  required="true" />
    </snd>
    <rcv>
        <field name="status" desc="状态 0:成功，1:失败" />
        <field name="num" desc="修改数量"/>
    </rcv>
</trans>