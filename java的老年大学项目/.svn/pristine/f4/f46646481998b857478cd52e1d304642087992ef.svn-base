package com.learnyeai.testing.api.vo;

import java.io.Serializable;

/**
 * 题目
 *
 * @author twang
 */
public class OptionVo implements Serializable {

	private static final long serialVersionUID = -6465718597500372046L;

	public OptionVo() {

	}

	public OptionVo(String itemId, String optionContent, Integer orderNum, String checked) {
		super();
		this.itemId = itemId;
		this.optionContent = optionContent;
		this.orderNum = orderNum;
		this.checked = checked;
	}

	/**
	 * ID
	 */
	private String itemId;

	/**
	 * 题支内容
	 */
	private String optionContent;
	/**
	 * 序号
	 */
	private Integer orderNum;

	/**
	 * 用户是否选中了该题
	 */
	private String checked = "0";

	public String getItemId() {
		return itemId;
	}

	public void setItemId(String itemId) {
		this.itemId = itemId;
	}

	public String getOptionContent() {
		return optionContent;
	}

	public void setOptionContent(String optionContent) {
		this.optionContent = optionContent;
	}

	public Integer getOrderNum() {
		return orderNum;
	}

	public void setOrderNum(Integer orderNum) {
		this.orderNum = orderNum;
	}

	public String getChecked() {
		return checked;
	}

	public void setChecked(String checked) {
		this.checked = checked;
	}
}
