<?xml version="1.0" encoding="utf-8"?>
<trans name="queryResources" desc="学员多个资源id的学习情况列表">
	<snd>
		<field name="studyUserId" desc="学习人id" required="true" />
		<field name="resId" desc="资源id 多个逗号隔开" required="true" />
		<field name="resinfo" desc="是否需要资源信息  0/1" />
		<field name="orderType" desc="排序方式   0:完成状态  (进行中，已完成) 1：最近学习时间倒排   2:选课时间顺排    3选课时间倒排" />
	</snd>
	<rcv>
		<field-list name="LIST" type="E" desc="客户学习资源列表">
			<field name="lresId" desc="id" />
			<field name="studyUserId" desc="学习人id" />
			<field name="resId" desc="资源id" />
			<field name="resName" desc="资源名称" />
			<field name="source" desc="来源0:资源;1:课程" />
			<field name="studyTime" desc="学习时长" />
			<field name="studyNum" desc="学习次数" />
			<field name="studyBreakpoint" desc="学习断点" />
			<field name="studyFinished" desc="是否完成" />
			<field name="createDate" desc="创建时间" />
			<field name="updateDate" desc="更新时间" />
			<field name="siteId" desc="站点" />
			<field name="mchtId" desc="商户id" />
			<field name="mchtSchmId" desc="商户方案id" />
		</field-list>
	</rcv>
</trans>