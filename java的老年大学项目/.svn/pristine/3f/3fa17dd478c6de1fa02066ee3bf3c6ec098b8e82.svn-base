        this.schmEdtPhoto = schmEdtPhoto;
    }
    public String getSchmEdtStatus() {
        return schmEdtStatus;
    }

    public void setSchmEdtStatus(String schmEdtStatus) {
        this.schmEdtStatus = schmEdtStatus;
    }
