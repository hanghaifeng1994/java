<?xml version="1.0" encoding="utf-8"?>
<trans name="queryPageByCatIds" desc="资讯管理-列表">
    <snd>
        <field name="catId" desc="栏目id 多个栏目id用英文逗号隔开"/>
        <field name="siteId" desc="站点"/>
    </snd>
    <rcv>
        <field-list name="LIST" desc="资讯管理-列表" type="E">
            <field name="articleId" desc="ID"/>
            <field name="title" desc="标题"/>
            <field name="link" desc="链接"/>
            <field name="color" desc="标题颜色"/>
            <field name="photo" desc="封面"/>
            <field name="keywords" desc="关键字"/>
            <field name="description" desc="描述、摘要"/>
            <field name="weight" desc="权重"/>
            <field name="weightDate" desc="权重日期"/>
            <field name="posid" desc="位置标识"/>
            <field name="copyfrom" desc="来源"/>
            <field name="author" desc="作者"/>
            <field name="allowComment" desc="是否允许评论"/>
            <field name="content" desc="文章内容"/>
            <field name="relation" desc="相关文章"/>
            <field name="fileIds" desc="附件ids"/>
            <field name="fileIdNames" desc="附件名称"/>
            <field name="status" desc="状态：0未提交、1审核中、2审核通过、3审核失败"/>
            <field name="pubStatus" desc="0未发布、1已发布"/>
            <field name="auditId" desc="审核id"/>
            <field name="articleBit" desc="是否加精"/>
            <field name="createBy" desc="创建人"/>
            <field name="createDate" desc="创建时间"/>
            <field name="updateBy" desc="更新人"/>
            <field name="updateDate" desc="更新时间"/>
            <field name="delFlag" desc="删除标记"/>
            <field name="siteId" desc="站点"/>
            <field name="mchtId" desc="商户id"/>
            <field name="mchtSchmId" desc="商户方案id"/>
            <field name="newsManageStatus" desc="管理状态 管理状态为1 可以进行取消发布操作"/>
        </field-list>
    </rcv>
</trans>