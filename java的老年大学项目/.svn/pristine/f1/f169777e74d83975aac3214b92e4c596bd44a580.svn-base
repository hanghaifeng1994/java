<?xml version="1.0" encoding="utf-8"?>
<trans name="detail" desc="订单-详情">
    <snd>
        <field name="orderId" desc="id"/>
    </snd>
    <rcv>
        <field name="orderId" desc="订单id"/>
        <field name="name" desc="名称"/>
        <field name="sn" desc="订单号"/>
        <field name="userId" desc="下单人id"/>
        <field name="userName" desc="下单人姓名"/>
        <field name="idcard" desc="下单人身份证"/>
        <field name="batchId" desc="批次id"/>
        <field name="type" desc="订单类型1:个人订单;2:集体订单"/>
        <field name="orderType" desc="订单类型1:购买课程;2:班级报名"/>
        <field name="status" desc="文件格式"/>
        <field name="needPrice" desc="订单应收"/>
        <field name="readPrice" desc="订单实收"/>
        <field name="discount" desc="状态：0未提交、1审核中、2审核通过、3审核失败、4不需要审核"/>
        <field name="callbackUrl" desc="回调url"/>
        <field name="payType" desc="支付方式1:线上;2:线下"/>
        <field name="upopSn" desc="给支付平台的订单号"/>
        <field name="upopSuccess" desc="支付平台是否支付成功"/>
        <field name="queryNum" desc="到支付平台的查询次数"/>
        <field name="xyk" desc="是否信用卡"/>
        <field name="payAppType" desc="下单应用类型（web/app/wxmapp）"/>
        <field name="factPayAppType" desc="支付应用类型（web/app/wxmapp）"/>
        <field name="fqStatus" desc="是否废弃订单"/>
        <field name="payPath" desc="支付平台(wx/ali/union)"/>
        <field name="paySource" desc="支付途径1:银行付款;2:现场支付;3:在线支付"/>
        <field name="remark" desc="订单付款备注"/>
        <field name="payOperator" desc="支付操作者"/>
        <field name="endTime" desc="订单结束时间"/>
        <field name="upopTime" desc="到支付平台的订单时间"/>
        <field name="fqTime" desc="废弃时间或失效时间"/>
        <field name="payDate" desc="支付时间"/>
        <field name="createDate" desc="创建时间"/>
        <field name="updateDate" desc="更新时间"/>
        <field name="siteId" desc="站点"/>
        <field name="mchtId" desc="商户id"/>
        <field name="mchtSchmId" desc="商户方案id"/>
    </rcv>
</trans>