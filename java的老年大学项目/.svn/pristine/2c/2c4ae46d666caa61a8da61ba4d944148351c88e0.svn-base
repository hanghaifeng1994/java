    public String getActPubStatus() {
        return actPubStatus;
    }

    public void setActPubStatus(String actPubStatus) {
        this.actPubStatus = actPubStatus;
    }
    public Date getActPubDate() {
        return actPubDate;
    }

    public void setActPubDate(Date actPubDate) {
