<?xml version="1.0" encoding="utf-8"?>
<trans name="publish" desc="用户证书-批量发布/取消发布">
	<snd>
		<field name="ucId" desc="用户证书id 多个id用英文逗号隔开" required="true" />
		<field name="ucPubStatus" desc="发布状态  0/1" required="true" />
	</snd>
	<rcv>
		<field name="status" desc="状态 0:发布成功，1:发布失败" />
		<field name="num" desc="发布数量" />
	</rcv>
</trans>