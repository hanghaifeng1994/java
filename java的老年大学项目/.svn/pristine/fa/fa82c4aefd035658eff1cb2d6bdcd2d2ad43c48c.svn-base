        public static String delflag = "DELFLAG";  // 软删除标识
        public static String hasSubject = "HAS_SUBJECT";  // 是否有主观题
        public static String excellentScore = "EXCELLENT_SCORE";  // 优秀线
        public static String higherPercent = "HIGHER_PERCENT";  // 高等百分比
        public static String lowerPercent = "LOWER_PERCENT";  // 低等百分比
        public static String mediumPercent = "MEDIUM_PERCENT";  // 中等百分比
        public static String qualifiedScore = "QUALIFIED_SCORE";  // 合格线
        public static String pcDisplayType = "PC_DISPLAY_TYPE";  // PC端题目显示类型（单题/整卷/分题型)
        public static String mobileDisplayType = "MOBILE_DISPLAY_TYPE";  // 移动端题目显示类型（单体/整卷/分题型)
        public static String userType = "USER_TYPE";  // 使用类型（练习/考试)lx/ks
        public static String createBy = "CREATE_BY";  // 创建人
        public static String createDate = "CREATE_DATE";  // 创建时间
        public static String updateBy = "UPDATE_BY";  // 更新人
