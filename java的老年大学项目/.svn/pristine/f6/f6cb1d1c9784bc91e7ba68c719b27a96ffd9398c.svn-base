<?xml version="1.0" encoding="utf-8"?>
<trans name="save" desc="课程-新增/编辑">
	<snd>
		<field name="csId" desc="id" />
		<field name="csCatstr" desc="课程分类id，逗号隔开" />
		<field name="csName" desc="名称" />
		<field name="csSerialNo" desc="课程编号" />
		<field name="csTag" desc="标签" />
		<field name="csInfo" desc="简介" />
		<field name="csPhoto" desc="封面" />
		<field name="csImgs" desc="配图" />
		<field name="csPoints" desc="积分" />
		<field name="csPrice" desc="价格" />
		<field name="csStudylength" desc="学分/学时" />
		<field name="csLecturer" desc="讲师" />
		<field name="csCopyrightStartdate" desc="有效期开始时间" />
		<field name="csCopyrightEnddate" desc="有效期结束时间" />
